config wifi-device 'radio0'
        option type 'mac80211'
        option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0'
        option country 'DE'
        option htmode 'HT40'
        option channel '44'
        option txpower '15'

config wifi-device 'radio1'
        option type 'mac80211'
        option hwmode '11g'
	option path 'platform/ar934x_wmac'
        option country 'DE'
        option channel '11'
	option htmode 'HT20'
        option txpower '20'

config wifi-iface 'adhoc24'
        option network 'mesh_wifi24'
        option device 'radio1'
        option ifname 'adhoc24'
        option mode 'adhoc'
        option ssid 'wifimesh-g-ac'
        option bssid 'CA:FF:24:FF:AC:24'
        option encryption 'none'
        option mcast_rate '12000'
	option disabled '0'

config wifi-iface 'adhoc5'
        option network 'mesh_wifi5'
        option device 'radio0'
        option ifname 'adhoc5'
        option mode 'adhoc'
        option ssid 'wifimesh-a-ac'
        option bssid 'CA:FE:52:FF:AC:52'
        option encryption 'none'
        option mcast_rate '12000'
	option disabled '0'

config wifi-iface 'appub24'
        option device 'radio1'
        option mode 'ap'
        option ifname 'appub24'
        option ssid 'Freifunk'
        option encryption 'none'
        option network 'guests'
	option disabled '0'

config wifi-iface 'appub5'
        option device 'radio0'
        option mode 'ap'
        option ifname 'appub5'
        option ssid 'Freifunk'
        option encryption 'none'
        option network 'guests'
	option disabled '0'

config wifi-iface 'appriv24'
        option device 'radio1'
        option mode 'ap'
        option ssid 'changemySSIDandKEY'
        option network 'lan'
        option encryption 'psk2'
        option key 'sdfjkslj(9h4839h8)H8'
	option disabled '1'
config wifi-iface 'appriv5'
        option device 'radio0'
        option mode 'ap'
        option ssid 'changemySSIDandKEY'
        option network 'lan'
        option encryption 'psk2'
        option key 'sdfjkslj(9h4839h8)H8'
	options disabled '1'
