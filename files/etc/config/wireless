
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:01/0000:01:00.0'
	option htmode 'VHT80'
	option txpower '17'
	option country 'DE'
	option channel 'auto'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ifname 'appriv'
	option ssid 'Spaltung'
	option encryption 'wpa2'
	option auth_server '10.10.101.49'
	option auth_secret '7tndOgQzFJiCNFSTpmls'
	option acct_server '10.10.101.49'
	option acct_secret '7tndOgQzFJiCNFSTpmls'
	option nasid 'c7basement'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option country 'DE'
	option channel '11'
	option txpower '20'

config wifi-iface 'mesh_guests'
	option device 'radio1'
	option mode 'ap'
	option ifname 'appub'
	option ssid 'Freifunk'
	option encryption 'none'
	option network 'mesh'
	option disabled '1'

config wifi-iface 'mesh_spine'
	option network 'mesh_spine'
	option device 'radio1'
	option ifname 'adhoc0'
	option mode 'adhoc'
	option ssid 'wifimesh-g-ac'
	option bssid 'CA:FF:24:FF:AC:24'
	option encryption 'none'
	option mcast_rate '12000'
	option disabled '1'

config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option ssid 'Spaltung'
	option network 'lan'
	option auth_server '10.10.101.49'
	option auth_secret '7tndOgQzFJiCNFSTpmls'
	option acct_server '10.10.101.49'
	option acct_secret '7tndOgQzFJiCNFSTpmls'
	option nasid 'c7basement'
	option encryption 'wpa2'

