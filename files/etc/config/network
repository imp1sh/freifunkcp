
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fdd5:fdc6:6d9f::/48'

config interface 'lan'
	option ifname 'eth0.5'
	option type 'bridge'
	option _orig_ifname 'eth0.5 appriv'
	option _orig_bridge 'true'
	option proto 'dhcp'

config interface 'wan'
	option ifname 'eth0.3'
	option proto 'dhcp'

config interface 'wan6'
	option ifname 'eth0.3'
	option proto 'dhcpv6'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'
	option mirror_source_port '0'
	option mirror_monitor_port '0'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option vid '5'
	option ports '0t 2 5t'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option vid '3'
	option ports '1 5t 6'

config interface 'mesh_spine'
	option ifname 'adhoc24ghz'
	option mtu '1528'
	option proto 'batadv'
	option mesh 'bat0'

config interface 'mesh_cable'
	option ifname 'eth0.11'
	option mtu '1528'
	option proto 'batadv'
	option mesh 'bat0'

config interface 'mesh'
	option type 'bridge'
	option _orig_ifname 'appub'
	option _orig_bridge 'true'
	option proto 'dhcpv6'
	option ifname 'appub bat0'
	option reqaddress 'try'
	option reqprefix 'auto'

config switch_vlan
	option device 'switch0'
	option vlan '3'
	option vid '11'
	option ports '0t 3 4 5t'

