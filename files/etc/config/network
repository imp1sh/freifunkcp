
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fdff:31c3:21::/48'

config interface 'lan'
	option type 'bridge'
	option _orig_ifname 'eth1 appriv'
	option _orig_bridge 'true'
	option proto 'static'
	option ifname 'appriv eth1.5'
	option netmask '255.255.255.0'
	option ipaddr '10.10.101.34'
	option gateway '10.10.101.49'

config interface 'mesh_spine'
	option ifname 'adhoc0'
	option mtu '1528'
	option proto 'batadv'
	option mesh 'bat0'

config interface 'mesh_cable'
	option ifname 'eth1.11'
	option mtu '1528'
	option proto 'batadv'
	option mesh 'bat0'

config interface 'wan'
	option proto 'dhcp'
	option _orig_ifname 'eth0'
	option _orig_bridge 'true'
	option ifname 'eth0'

config interface 'wan6'
	option ifname 'eth0'
	option proto 'dhcpv6'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'
	option mirror_source_port '0'
	option mirror_monitor_port '0'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option vid '3'
	option ports '1 5t 6'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option vid '5'
	option ports '0t 2 5t'

config interface 'mesh'
	option type 'bridge'
	option _orig_ifname 'appub'
	option _orig_bridge 'true'
	option ifname 'bat0 appub'
	option proto 'dhcpv6'
	option reqaddress 'try'
	option reqprefix 'auto'

config switch_vlan
	option device 'switch0'
	option vlan '3'
	option vid '11'
	option ports '0t 3 4 5t'

